VITE_API_BASE_URL=/api
VITE_APP_NAME=Qyreach
VITE_APP_VERSION=1.0.0

# Development Authentication (for testing with backend)
VITE_DEV_AUTH_TOKEN=dev-test-token-12345
VITE_DEV_COMPANY_ID=dev-company-12345
VITE_BACKEND_URL=http://localhost:3000

# Fallback API (only enable when backend is completely unavailable)
# Set to 'true' only for development when no backend server is running or for testing
VITE_ENABLE_FALLBACK_API=false

# Force backend connection - disable mock API completely
VITE_FORCE_BACKEND=true

